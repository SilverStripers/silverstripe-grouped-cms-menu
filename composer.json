{
	"name": "symbiote/silverstripe-grouped-cms-menu",
	"description": "Allows you to group CMS menu items.",
	"type": "silverstripe-vendormodule",
	"homepage": "https://github.com/symbiote/silverstripe-grouped-cms-menu",
	"keywords": [
		"silverstripe",
		"menu",
		"module",
		"cms"
	],
	"license": "BSD-3-Clause",
	"authors": [{
		"name": "Marcus Nyeholt",
		"email": "marcus@symbiote.com.au"
	}],
	"require": {
		 "silverstripe/admin": "~1.3"
	},
	"extra": {
        "expose": [
            "client/dist"
        ],
        "installer-name": "grouped-cms-menu"
	},
	"replace": {
		"silverstripe-australia/grouped-cms-menu": "self.version"
	},
    "autoload": {
        "psr-4": {
            "Symbiote\\GroupedCMSMenu\\": "src/"
        }
    },
	"minimum-stability": "dev"
}
